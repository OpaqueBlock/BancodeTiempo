<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DocumentosUsuario">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="23dp"
        android:text="@string/ine"
        app:layout_constraintBottom_toTopOf="@+id/boton_documento_ine"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="21dp"
        android:text="@string/comprobanteDomicilio"
        app:layout_constraintBottom_toTopOf="@+id/boton_compro_domicilio"
        app:layout_constraintStart_toStartOf="@+id/boton_documento_ine"
        app:layout_constraintTop_toBottomOf="@+id/boton_documento_ine" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginBottom="23dp"
        android:text="@string/certificado"
        app:layout_constraintBottom_toTopOf="@+id/boton_certificado"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/boton_compro_domicilio" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginBottom="21dp"
        android:text="@string/cartaPenal"
        app:layout_constraintBottom_toTopOf="@+id/boton_penal_xd"
        app:layout_constraintStart_toStartOf="@+id/boton_penal_xd"
        app:layout_constraintTop_toBottomOf="@+id/boton_certificado" />

    <Button
        android:id="@+id/boton_penal_xd"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="258dp"
        android:backgroundTint="#313435"
        android:text="@string/eligeDoc"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        app:iconTint="#8C8888"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <Button
        android:id="@+id/boton_documento_ine"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="17dp"
        android:backgroundTint="#313435"
        android:text="@string/eligeDoc"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        app:iconTint="#8C8888"
        app:layout_constraintBottom_toTopOf="@+id/textView13"
        app:layout_constraintEnd_toStartOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Button
        android:id="@+id/boton_certificado"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="26dp"
        android:backgroundTint="#313435"
        android:text="@string/eligeDoc"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        app:iconTint="#8C8888"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintEnd_toStartOf="@+id/imageView6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView14" />

    <Button
        android:id="@+id/boton_compro_domicilio"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginStart="35dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="34dp"
        android:backgroundTint="#313435"
        android:text="@string/eligeDoc"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        app:iconTint="#8C8888"
        app:layout_constraintBottom_toTopOf="@+id/textView14"
        app:layout_constraintEnd_toStartOf="@+id/imageView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="276dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/boton_penal_xd"
        app:layout_constraintTop_toBottomOf="@+id/imageView6"
        app:srcCompat="@drawable/round_add_a_photo_black_48" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="82dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="75dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/boton_documento_ine"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/round_add_a_photo_black_48" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="84dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/boton_certificado"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:srcCompat="@drawable/round_add_a_photo_black_48" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="31dp"
        android:layout_marginBottom="94dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/boton_compro_domicilio"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:srcCompat="@drawable/round_add_a_photo_black_48" />

</androidx.constraintlayout.widget.ConstraintLayout>